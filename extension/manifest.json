{
  "name": "Import",
  "short_name": "Import",
  "description":
    "Import / Inject / require javascript modules in your console.",
  "version": "0.0.3",
  "manifest_version": 2,
  "icons": {
    "128": "assets/icons/128.png"
  },
  "permissions": [
    "https://api.cdnjs.com/libraries/*/*",
    "https://cdnjs.cloudflare.com/*/*",
    "https://*/*",
    "http://*/*",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_title": "Click to activate"
  },
  "background": {
    "scripts": ["background.js", "loadScript.js"],
    "persistent": false
  },
  "web_accessible_resources": ["script.js"],
  "content_security_policy":
    "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'"
}
