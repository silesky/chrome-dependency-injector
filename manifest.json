{
  "name": "Console",
  "short_name": "Console",
  "description":
    "This extension shows how to evaluate script in the content scope",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "https://api.cdnjs.com/libraries/*/*",
    "https://cdnjs.cloudflare.com/*/*",
    "https://*/*",
    "http://*/*",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_title": "Click to activate"
  },
  "background": {
    "scripts": ["background.js", "loadScript.js"],
    "persistent": false
  },
  "web_accessible_resources": ["script.js"],
  "content_security_policy":
    "script-src 'self' https://cdnjs.cloudflare.com; object-src 'self'"
}
